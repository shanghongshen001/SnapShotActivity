<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.snapshotactivity"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.ayj.snapshotactivity.SnapShotActivity"
            android:label="@string/title_activity_snap_shot" >
        </activity>
        <activity
            android:name="com.example.snapshotactivity.Form1Activity"
            android:label="交通运输行政强制措施审批表"
            android:windowSoftInputMode="adjustResize|stateVisible" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.snapshotactivity.PrintPreviewActivity"
            android:label="@string/title_activity_print_preview" >
        </activity>
    </application>

</manifest>